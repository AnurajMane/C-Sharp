@{
    <h3>Passed data from View by both ViewBag and ViewData</h3>


    <h4>Using ViewData</h4>
    <p>Student Name : @ViewData["name"]</p>
    <p>Student Age : @ViewData["age"]</p>


    <h4>Using ViewBag</h4>
    <p>Student City : @ViewBag.city</p>
    <p>Student Country : @ViewBag.country</p>





}
